{% extends '::base.html.twig' %}

{% block body -%}
    <h1>HotelRestaurante list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Direccion</th>
                <th>Servicios</th>
                <th>Tipo</th>
                <th>Direccionmapa</th>
                <th>Provincia</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('hotelrestaurante_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.nombre }}</td>
                <td>{{ entity.direccion }}</td>
                <td>{{ entity.servicios }}</td>
                <td>{{ entity.tipo }}</td>
                <td>{{ entity.direccionMapa }}</td>
                <td>{{ entity.provincia }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('hotelrestaurante_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('hotelrestaurante_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('hotelrestaurante_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
